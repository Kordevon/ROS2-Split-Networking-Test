<?xml version="1.0" encoding="UTF-8" ?>
<profiles xmlns="http://www.eprosima.com">
    <transport_descriptors>
        <transport_descriptor>
            <transport_id>CustomUdpTransportWhitelistAddress</transport_id>
            <type>UDPv4</type>
            <interfaceWhiteList>
                <address>192.168.1.2</address>
                <address>192.168.1.1</address>
            </interfaceWhiteList>
        </transport_descriptor>
    </transport_descriptors>

    <participant profile_name="CustomUdpTransportWhitelistAddressParticipant">
        <rtps>
            <useBuiltinTransports></useBuiltinTransports>
            <userTransports>
                <transport_id>CustomUdpTransportWhitelistAddress</transport_id>
            </userTransports>
        </rtps>
    </participant>
</profiles>