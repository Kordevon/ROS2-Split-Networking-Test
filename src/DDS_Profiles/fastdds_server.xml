<?xml version="1.0" encoding="UTF-8" ?>
<profiles xmlns="http://www.eprosima.com/XMLSchemas/fastrtps_profiles">
    <participant profile_name="ros_host_native_profile" is_default_profile="true">
        <rtps>
            <builtin>
                <initialPeersList>
                    <locator><udpv4><address>192.168.1.10</address></udpv4></locator>
                    <locator><udpv4><address>192.168.2.10</address></udpv4></locator>
                    <locator><udpv4><address>192.168.1.15</address></udpv4></locator>
                    <locator><udpv4><address>192.168.2.15</address></udpv4></locator>
                </initialPeersList>
                <discovery_config>
                    <discoveryProtocol>SIMPLE</discoveryProtocol>
                    <use_multicast>false</use_multicast>
                </discovery_config>
            </builtin>
            <transport_descriptors>
                <transport_descriptor>
                    <transport_id>udp_host_combined</transport_id>
                    <type>UDPv4</type>
                    <interfaceWhiteList>
                        <address>192.168.1.10</address>
                        <address>192.168.2.10</address>
                        </interfaceWhiteList>
                </transport_descriptor>
            </transport_descriptors>
            <userTransports>
                <transport_id>udp_host_combined</transport_id>
            </userTransports>
            <qos>
                <publishMode><kind>ASYNCHRONOUS_PUBLISH_MODE</kind></publishMode>
            </qos>
        </rtps>
    </participant>
</profiles>